aside>\r\n    </el-main>\r\n  </el-container>\r\n</template>\r\n\r\n<script>\r\nimport barchart from \"../components/barChart.vue\";\r\nimport piechart from \"../components/pieChart.vue\";\r\nimport ringchart from \"../components/ringChart.vue\";\r\nimport righttop from \"../components/homeRightTop.vue\";\r\nimport tab from \"../components/recordTable.vue\";\r\nimport rcd from \"../components/dataRecord.vue\"\r\nimport lst from \"../components/dataList.vue\"\r\nimport fot from \"../components/homeFooter.vue\"\r\nexport default {\r\n  data() {\r\n    return {};\r\n  },\r\n  methods: {},\r\n  components: {\r\n    // A:B，A表示在上方中所展示的组件名\r\n    // B表示在components文件夹下对应的组件名\r\n    bar: barchart,\r\n    pie: piechart,\r\n    ring: ringchart,\r\n    choose: righttop,\r\n    tab: tab,\r\n    rcd:rcd,\r\n    lst:lst,\r\n    fot:fot,\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n@import \"../assets/css/Index_css.css\";\r\n// 引入 Index_left 中图表的样式\r\n@import \"../assets/css/Index_charts.css\";\r\n// 引入成绩样式\r\n@import \"../assets/css/Index_score.css\";\r\n\r\n// 貌似关于 element 的标签无法用 import 使用\r\n.el-menu {\r\n  height: 100%;\r\n}\r\n//设置了行高和字体大小、高度颜色\r\n.el-menu-item {\r\n  font-size: 18px;\r\n  height: 100% !important;\r\n}\r\n//设置选中el-menu-item时的状态\r\n// 这里不显示\r\n// 后面再改这两个\r\n.el-menu-item .is-active {\r\n  border-bottom-color: #000 !important;\r\n}\r\n//设置选中el-menu-item时的样式\r\n.el-menu-item:hover {\r\n}\r\n\r\n.el-button--u_n_l {\r\n  background-color: #173456;\r\n  margin: 0;\r\n  padding: 0;\r\n  border: 0;\r\n  font-size: large;\r\n  color: #fff;\r\n}\r\n\r\n.el-button--u_n_l:hover {\r\n  background-color: #173456;\r\n}\r\n.record{\r\n  width: 100%;\r\n  position: relative;\r\n  margin-top:30px;\r\n}\r\n.form_table {\r\n  width: 100%;\r\n  position: absolute;\r\n  bottom:0\r\n}\r\n.form_list{\r\n  color: blue;\r\n  font-size:20px;\r\n}\r\n</style>\r\n"]}]}