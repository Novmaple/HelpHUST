架</el-button>　\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport data from '../../../../public/static/hust_data.json'\r\nexport default {\r\n  data() {\r\n    return {\r\n      typeoptions: [\r\n        {\r\n          typevalue: \"日用品\",\r\n          label: \"日用品\",\r\n        },\r\n        {\r\n          typevalue: \"电器\",\r\n          label: \"电器\",\r\n        },\r\n        {\r\n          typevalue: \"书籍\",\r\n          label: \"书籍\",\r\n        },\r\n      ],\r\n      goods_info: [\r\n        {\r\n          id: 1,\r\n          name: \"某商品\",\r\n          price: \"10元\",\r\n          type: \"日用品\",\r\n          s_time: \"2021-11-1\",\r\n          e_time: \"2021-11-2\",\r\n          seller: \"张三\",\r\n          seller_id: 1,\r\n          buyer: \"李四\",\r\n          buyer_id: 2,\r\n          description: \"很好\",\r\n        },\r\n        {\r\n          id: 2,\r\n          name: \"某商品\",\r\n          price: \"5元\",\r\n          type: \"书籍\",\r\n          s_time: \"2021-11-2\",\r\n          e_time: \"2021-11-3\",\r\n          seller: \"张三\",\r\n          seller_id: 1,\r\n          buyer: \"李四\",\r\n          buyer_id: 2,\r\n          description: \"很好\",\r\n        },\r\n      ],\r\n      pickerOptions: {\r\n        disabledDate(time) {\r\n          return time.getTime() > Date.now();\r\n        },\r\n        shortcuts: [\r\n          {\r\n            text: \"今天\",\r\n            onClick(picker) {\r\n              picker.$emit(\"pick\", new Date());\r\n            },\r\n          },\r\n          {\r\n            text: \"昨天\",\r\n            onClick(picker) {\r\n              const date = new Date();\r\n              date.setTime(date.getTime() - 3600 * 1000 * 24);\r\n              picker.$emit(\"pick\", date);\r\n            },\r\n          },\r\n          {\r\n            text: \"一周前\",\r\n            onClick(picker) {\r\n              const date = new Date();\r\n              date.setTime(date.getTime() - 3600 * 1000 * 24 * 7);\r\n              picker.$emit(\"pick\", date);\r\n            },\r\n          },\r\n        ],\r\n      },\r\n      name: \"\",\r\n      minprice: \"\",\r\n      maxprice: \"\",\r\n      typevalue: \"\",\r\n      datevalue: \"\",\r\n    };\r\n  },\r\n  mounted(){\r\n    this.read()\r\n  },\r\n  methods: {\r\n    read(){\r\n      this.goods_info=data.goods_info;\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.select {\r\n  text-align: center;\r\n}\r\n\r\n.list {\r\n  margin-top: 2%;\r\n}\r\n.s_list {\r\n  height: 60%;\r\n  width: 80%;\r\n}\r\n</style>"]}]}