  default-active=\"2\"\r\n        >\r\n          <el-menu-item index=\"1\">数字资产展示</el-menu-item>\r\n          <el-menu-item index=\"2\">设计协同管理</el-menu-item>\r\n          <el-menu-item index=\"3\">智慧施工管理</el-menu-item>\r\n          <el-menu-item index=\"4\">智慧运营维护</el-menu-item>\r\n          <el-menu-item index=\"5\">智慧文创旅游</el-menu-item>\r\n        </el-menu>\r\n      </div>\r\n      <div class=\"user_logout\">\r\n        <el-button icon=\"el-icon-user-solid\" size=\"medium\" type=\"u_n_l\"\r\n          >业主</el-button\r\n        >\r\n        <el-button icon=\"el-icon-switch-button\" size=\"medium\" type=\"u_n_l\"\r\n          >退出</el-button\r\n        >\r\n      </div>\r\n    </el-header>\r\n    <el-main>\r\n      <el-aside class=\"aside_left\">\r\n        <div id=\"composition_histogram\" class=\"histogram\"></div>\r\n        <div id=\"composition_doughnut\" class=\"doughnut\"></div>\r\n        <div id=\"composition_pie\" class=\"pie\"></div>\r\n      </el-aside>\r\n      <el-footer> footer </el-footer>\r\n      <el-aside class=\"aside_right\">\r\n        <div id=\"versions\" class=\"select_part\">\r\n          <el-select v-model=\"value\" placeholder=\"请选择\">\r\n            <el-option\r\n              v-for=\"item in options\"\r\n              :key=\"item.value\"\r\n              :label=\"item.label\"\r\n              :value=\"item.value\"\r\n            >\r\n            </el-option>\r\n          </el-select>\r\n        </div>\r\n      </el-aside>\r\n    </el-main>\r\n  </el-container>\r\n</template>\r\n\r\n<script>\r\n// 基于准备好的dom，初始化echarts实例\r\nvar myChart = echarts.init(document.getElementById(\"main\"));\r\n\r\n// 指定图表的配置项和数据\r\nvar option = {\r\n  title: {\r\n    text: \"ECharts 入门示例\",\r\n  },\r\n  tooltip: {},\r\n  legend: {\r\n    data: [\"销量\"],\r\n  },\r\n  xAxis: {\r\n    data: [\"衬衫\", \"羊毛衫\", \"雪纺衫\", \"裤子\", \"高跟鞋\", \"袜子\"],\r\n  },\r\n  yAxis: {},\r\n  series: [\r\n    {\r\n      name: \"销量\",\r\n      type: \"bar\",\r\n      data: [5, 20, 36, 10, 10, 20],\r\n    },\r\n  ],\r\n};\r\n\r\n// 使用刚指定的配置项和数据显示图表。\r\nmyChart.setOption(option);\r\nexport default {\r\n  data() {\r\n    return {\r\n      options: [\r\n        {\r\n          value: \"选项1\",\r\n          label: \"黄金糕\",\r\n        },\r\n        {\r\n          value: \"选项2\",\r\n          label: \"双皮奶\",\r\n        },\r\n        {\r\n          value: \"选项3\",\r\n          label: \"蚵仔煎\",\r\n        },\r\n        {\r\n          value: \"选项4\",\r\n          label: \"龙须面\",\r\n        },\r\n        {\r\n          value: \"选项5\",\r\n          label: \"北京烤鸭\",\r\n        },\r\n      ],\r\n      value: \"\",\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n@import \"../assets/css/Index_css.css\";\r\n// 引入 Index_left 中图表的样式\r\n@import \"../assets/css/Index_charts.css\";\r\n// 引入成绩样式\r\n@import \"../assets/css/Index_score.css\";\r\n\r\n// 貌似关于 element 的标签无法用 import 使用\r\n.el-menu {\r\n  height: 100%;\r\n}\r\n//设置了行高和字体大小、高度颜色\r\n.el-menu-item {\r\n  font-size: 18px;\r\n  height: 100% !important;\r\n}\r\n//设置选中el-menu-item时的状态\r\n// 这里不显示\r\n// 后面再改这两个\r\n.el-menu-item .is-active {\r\n  border-bottom-color: #000 !important;\r\n}\r\n//设置选中el-menu-item时的样式\r\n.el-menu-item:hover {\r\n}\r\n\r\n.el-button--u_n_l {\r\n  background-color: #173456;\r\n  margin: 0;\r\n  padding: 0;\r\n  border: 0;\r\n  font-size: large;\r\n  color: #fff;\r\n}\r\n\r\n.el-button--u_n_l:hover {\r\n  background-color: #173456;\r\n}\r\n</style>\r\n"]}]}