</div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      chartInstance: null,\r\n      allData: null,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.iniChart();\r\n    this.getData();\r\n  },\r\n  methods: {\r\n    iniChart() {\r\n      this.chartInstance = this.$echarts.init(this.$refs.ring_ref);\r\n      const initOption = {};\r\n      this.chartInstance.setOption(initOption);\r\n    },\r\n    async getData() {\r\n      const { data: ret } = await this.$http.get(\r\n        \"https://result.eolinker.com/iwvT4g78dfde82d24f1a538e8e5ff9d7b8c9fef930893b2?uri=http://127.0.0.1/certificate-stats/latest-7-inday/\"\r\n      );\r\n      this.allData = eval(\"(\" + ret + \")\").playload;\r\n      console.log(this.allData);\r\n      this.updateChart();\r\n    },\r\n    updateChart() {\r\n      const day = this.allData.map((item) => {\r\n        return item.day;\r\n      });\r\n      const count = this.allData.map((item) => {\r\n        return item.count;\r\n      });\r\n      const dataOption = {\r\n        tooltip: {\r\n          trigger: \"item\",\r\n        },\r\n        legend: {\r\n          top: \"bottom\",\r\n        },\r\n        color: [\"rgb(234,125,60)\", \"rgb(48,214,255)\", \"rgb(59,142,236)\"],\r\n        series: [\r\n          {\r\n            name: \"Access From\",\r\n            type: \"pie\",\r\n            radius: [\"40%\", \"70%\"],\r\n            avoidLabelOverlap: false,\r\n            label: {\r\n              show: false,\r\n              position: \"center\",\r\n            },\r\n            emphasis: {\r\n              label: {\r\n                show: true,\r\n                fontSize: \"40\",\r\n                fontWeight: \"bold\",\r\n              },\r\n            },\r\n            labelLine: {\r\n              show: false,\r\n            },\r\n            itemStyle: {\r\n              borderRadius: 8,\r\n              normal: {\r\n                label: {\r\n                  show: true,\r\n                  formatter: \"{b} :  {d}%\",\r\n                },\r\n                labelLine: { show: true },\r\n              },\r\n            },\r\n            data: [\r\n              { value: 1.8, name: \"建设单位成员\" },\r\n              { value: 30.3, name: \"专家组成员\" },\r\n              { value: 67.9, name: \"设计单位成员\" },\r\n            ],\r\n          },\r\n        ],\r\n      };\r\n      this.chartInstance.setOption(dataOption);\r\n    },\r\n    screenAdapter() {\r\n      const refHeight = this.$refs.ring_ref.offsetHeight;\r\n      const refWidth = this.$refs.ring_ref.offsetWidth;\r\n      let HW = [0, 0];\r\n      if (refHeight / refWidth > 264 / 474) {\r\n        // 当高占比为100%时\r\n        HW = [0.95, 1.7, (refHeight / 100) * 7.2];\r\n      } else {\r\n        HW = [0.9, 1.7, (refWidth / 100) * 4];\r\n      }\r\n      const adapterOption = {\r\n        title: {\r\n          textStyle: {\r\n            fontSize: HW[2],\r\n          },\r\n          top: HW[2] * HW[0],\r\n          left: HW[2] * HW[1],\r\n        },\r\n        series: [\r\n          {\r\n            barWidth: HW[2],\r\n          },\r\n        ],\r\n      };\r\n      this.chartInstance.setOption(adapterOption);\r\n      this.chartInstance.resize();\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n</style>"],"sourceRoot":"src/components"}]}