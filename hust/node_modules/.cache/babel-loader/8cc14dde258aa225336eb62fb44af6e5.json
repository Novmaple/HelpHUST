        <el-form-item label=\"类型\">\r\n          <el-select\r\n            v-model=\"typevalue\"\r\n            placeholder=\"请选择\"\r\n            style=\"width: 110px\"\r\n          >\r\n            <el-option\r\n              v-for=\"item in typeoptions\"\r\n              :key=\"item.typevalue\"\r\n              :label=\"item.label\"\r\n              :value=\"item.typevalue\"\r\n            >\r\n            </el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"选择时间\">\r\n          <el-date-picker\r\n            style=\"width: 140px\"\r\n            v-model=\"datevalue\"\r\n            align=\"right\"\r\n            type=\"date\"\r\n            placeholder=\"选择日期\"\r\n            :picker-options=\"pickerOptions\"\r\n          >\r\n          </el-date-picker>\r\n        </el-form-item>\r\n        <el-button type=\"primary\" plain>查询</el-button>\r\n      </el-form>\r\n    </div>\r\n    <div class=\"list\">\r\n      <el-table :data=\"goods_info\" height=\"250\" border style=\"width: 100%\">\r\n        <el-table-column prop=\"name\" label=\"商品名称\" align=\"center\">\r\n        </el-table-column>\r\n        <el-table-column prop=\"price\" label=\"价格\" align=\"center\">\r\n        </el-table-column>\r\n        <el-table-column prop=\"type\" label=\"类型\" align=\"center\">\r\n        </el-table-column>\r\n        <el-table-column prop=\"s_time\" label=\"上架时间\" align=\"center\">\r\n        </el-table-column>\r\n        <el-table-column label=\"操作\" align=\"center\" width=\"300px\">\r\n          <template>\r\n            <el-button type=\"primary\">详细信息</el-button>\r\n            <el-button type=\"danger\">下架</el-button>　\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport data from '../../../../public/static/hust_data.json'\r\nexport default {\r\n  data() {\r\n    return {\r\n      typeoptions: [\r\n        {\r\n          typevalue: \"日用品\",\r\n          label: \"日用品\",\r\n        },\r\n        {\r\n          typevalue: \"电器\",\r\n          label: \"电器\",\r\n        },\r\n        {\r\n          typevalue: \"书籍\",\r\n          label: \"书籍\",\r\n        },\r\n      ],\r\n      goods_info: [\r\n        {\r\n          id: 1,\r\n          name: \"某商品\",\r\n          price: \"10元\",\r\n          type: \"日用品\",\r\n          s_time: \"2021-11-1\",\r\n          e_time: \"2021-11-2\",\r\n          seller: \"张三\",\r\n          seller_id: 1,\r\n          buyer: \"李四\",\r\n          buyer_id: 2,\r\n          description: \"很好\",\r\n        },\r\n        {\r\n          id: 2,\r\n          name: \"某商品\",\r\n          price: \"5元\",\r\n          type: \"书籍\",\r\n          s_time: \"2021-11-2\",\r\n          e_time: \"2021-11-3\",\r\n          seller: \"张三\",\r\n          seller_id: 1,\r\n          buyer: \"李四\",\r\n          buyer_id: 2,\r\n          description: \"很好\",\r\n        },\r\n      ],\r\n      pickerOptions: {\r\n        disabledDate(time) {\r\n          return time.getTime() > Date.now();\r\n        },\r\n        shortcuts: [\r\n          {\r\n            text: \"今天\",\r\n            onClick(picker) {\r\n              picker.$emit(\"pick\", new Date());\r\n            },\r\n          },\r\n          {\r\n            text: \"昨天\",\r\n            onClick(picker) {\r\n              const date = new Date();\r\n              date.setTime(date.getTime() - 3600 * 1000 * 24);\r\n              picker.$emit(\"pick\", date);\r\n            },\r\n          },\r\n          {\r\n            text: \"一周前\",\r\n            onClick(picker) {\r\n              const date = new Date();\r\n              date.setTime(date.getTime() - 3600 * 1000 * 24 * 7);\r\n              picker.$emit(\"pick\", date);\r\n            },\r\n          },\r\n        ],\r\n      },\r\n      name: \"\",\r\n      minprice: \"\",\r\n      maxprice: \"\",\r\n      typevalue: \"\",\r\n      datevalue: \"\",\r\n    };\r\n  },\r\n  mounted(){\r\n    this.read()\r\n  },\r\n  methods: {\r\n    read(){\r\n      this.goods_info=data.goods_info;\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.select {\r\n  text-align: center;\r\n}\r\n\r\n.list {\r\n  margin-top: 2%;\r\n}\r\n.s_list {\r\n  height: 60%;\r\n  width: 80%;\r\n}\r\n</style>"],"sourceRoot":"src/components/admin/shop_manage"}]}